{
  "name": "mini-cz",
  "version": "0.8.0",
  "packageManager": "pnpm@8.6.2",
  "author": "Ray <i@mk1.io> (@so1ve)",
  "description": "The minimal commitizen cli.",
  "keywords": [],
  "type": "module",
  "homepage": "https://github.com/CreateWheel/mini-cz#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CreateWheel/mini-cz.git"
  },
  "bugs": {
    "url": "https://github.com/CreateWheel/mini-cz/issues"
  },
  "license": "MIT",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./cli": {
      "types": "./dist/cli.d.ts",
      "import": "./dist/cli.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "bin": {
    "mcz": "./dist/cli.mjs"
  },
  "files": [
    "dist",
    "bin"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rimraf dist && pkgroll --minify",
    "watch": "pkgroll --watch"
  },
  "devDependencies": {
    "@types/prompts": "^2.4.4",
    "clerc": "^0.40.0",
    "execa": "^7.1.1",
    "kons": "^0.7.1",
    "prompts": "^2.4.2",
    "tasuku": "^2.0.1",
    "yajuu": "^0.1.1"
  }
}

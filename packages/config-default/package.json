  "type": "module",
  "description": "Mini-cz default config.",
  "keywords": [],
  "homepage": "https://github.com/CreateWheel/mini-cz#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CreateWheel/mini-cz.git"
  },
  "bugs": {
    "url": "https://github.com/CreateWheel/mini-cz/issues"
  },
  "license": "MIT",
